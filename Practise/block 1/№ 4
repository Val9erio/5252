import math

v = float(input())
angle = float(input())

g = 9.8
a = math.radians(angle)

t_flight = 2 * v * math.sin(a) / g
h_max = (v * v * math.sin(a) ** 2) / (2 * g)

print(h_max)

t = 0
while t <= t_flight:
    h = v * math.sin(a) * t - (g * t * t) / 2
    if h < 0:
        h = 0
    print(h)
    t += 1
